{
  "name": "writable-stream-and-promise",
  "version": "1.0.4",
  "description": "Pipe a stream into a Promise.",
  "keywords": [
    "stream",
    "promise",
    "writable"
  ],
  "main": "index.js",
  "scripts": {
    "clean": "rimraf coverage",
    "test": "eslint --ignore-path=.gitignore . && nyc --reporter=lcov --reporter=text mocha test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raymond-lam/writable-stream-and-promise.git"
  },
  "author": "Raymond Lam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raymond-lam/writable-stream-and-promise/issues"
  },
  "homepage": "https://github.com/raymond-lam/writable-stream-and-promise#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "eslint": "^6.0.0",
    "greenkeeper-lockfile": "^1.15.1",
    "mocha": "^6.0.2",
    "nyc": "^14.0.0",
    "rimraf": "^2.6.3"
  },
  "nyc": {
    "sourceMap": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "check-coverage": true,
    "temp-dir": "./coverage/.nyc_output"
  }
}
